<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
        >

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/user_name_1-1"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="24dp"
            android:text="Name"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="186dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="187dp"
            />

    <TextView
            android:text="Groops"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView16"
            tools:layout_editor_absoluteX="167dp"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/listview_1_1"
            />

    <TextView
            android:text="Bio:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView17"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp"
            app:layout_constraintTop_toBottomOf="@+id/user_name_1-1"
            android:layout_marginTop="30dp"
            />

    <androidx.recyclerview.widget.RecyclerView
            android:layout_width="411dp"
            android:layout_height="184dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/recycler_1-1"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/textView16"
            />

    <com.google.android.material.button.MaterialButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Message User"
            android:id="@+id/message_1-1"
            android:layout_marginEnd="143dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="143dp"
            app:layout_constraintTop_toBottomOf="@+id/recycler_1-1"
            android:layout_marginTop="20dp"
            />

    <TextView
            android:id="@+id/bio_1-1"
            android:gravity="left"
            android:layout_height="126dp"
            android:layout_width="363dp"
            android:text=""
            tools:layout_editor_absoluteX="35dp"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/textView17"
            android:isScrollContainer="true"
            android:lines="7"
            ></TextView>

    <TextView
            android:text="Interests:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="32dp"
            android:id="@+id/textView14"
            app:layout_constraintTop_toBottomOf="@+id/bio_1-1"
            android:layout_marginTop="20dp"
            />

    <androidx.recyclerview.widget.RecyclerView
            android:layout_width="395dp"
            android:layout_height="60dp"
            tools:layout_editor_absoluteX="8dp"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/textView14"
            android:id="@+id/listview_1_1"
            />

</androidx.constraintlayout.widget.ConstraintLayout>